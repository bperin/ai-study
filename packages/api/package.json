{
  "name": "@memorang/api",
  "version": "1.0.0",
  "scripts": {
    "prisma": "prisma db push && prisma generate",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "dev": "nest start --watch",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "codegen": "openapi-generator-cli generate -i ../../openapi.json -g typescript-fetch -o src/generated --additional-properties=typescriptThreePlus=true,modelPropertyNaming=camelCase --skip-validate-spec"
  },
  "dependencies": {
    "@genkit-ai/ai": "^1.25.0",
    "@genkit-ai/core": "^1.25.0",
    "@genkit-ai/googleai": "^1.25.0",
    "@google-cloud/storage": "^7.18.0",
    "@google/adk": "^0.1.3",
    "@google/genai": "^1.14.0",
    "@google/generative-ai": "^0.1.0",
    "@modelcontextprotocol/sdk": "^1.17.5",
    "@nestjs/common": "^10.0.0",
    "@nestjs/config": "^3.0.0",
    "@nestjs/core": "^10.0.0",
    "@nestjs/jwt": "^11.0.2",
    "@nestjs/passport": "^11.0.5",
    "@nestjs/platform-express": "^10.0.0",
    "@nestjs/swagger": "^7.1.0",
    "@opentelemetry/api": "^1.9.0",
    "@prisma/adapter-pg": "^7.1.0",
    "@prisma/client": "^7.1.0",
    "@types/bcrypt": "^6.0.0",
    "bcrypt": "^6.0.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "passport-jwt": "^4.0.1",
    "pdf-parse": "^1.1.1",
    "pg": "^8.16.3",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.8.0",
    "swagger-ui-express": "^5.0.0",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.0.0",
    "@nestjs/schematics": "^10.0.0",
    "@nestjs/testing": "^10.0.0",
    "@types/express": "^4.17.0",
    "@types/jest": "^29.5.0",
    "@types/multer": "^1.4.0",
    "@types/node": "^20.0.0",
    "@types/pdf-parse": "^1.1.0",
    "@types/pg": "^8.16.0",
    "@types/supertest": "^2.0.0",
    "prisma": "^7.1.0",
    "source-map-support": "^0.5.0",
    "supertest": "^6.0.0",
    "ts-loader": "^9.0.0",
    "ts-node": "^10.0.0",
    "tsconfig-paths": "^4.0.0"
  }
}
